# === Stage 1: Info Gathering (Data Generation / Search) ===
# Typically uses a powerful model with search capabilities (e.g., Qwen-Plus/Max via Dashscope)
GEN_API_BASE=https://dashscope.aliyuncs.com/compatible-mode/v1
GEN_API_KEY=your_dashscope_api_key_here
GEN_MODEL_NAME=qwen-plus

# === Stage 2: Chat (Conversation) ===
# Typically uses a local or specialized model (e.g., Ollama with custom LTY model)
CHAT_API_BASE=http://localhost:11434/v1
CHAT_API_KEY=ollama
CHAT_MODEL_NAME=lty_v6:7b

# === General Config ===
DEBUG=True

# === Embedding Config ===
# EMBEDDING_BACKEND: "local" (BGE-M3) or "cloud" (Dashscope)
EMBEDDING_BACKEND=cloud
EMBEDDING_MODEL_NAME=text-embedding-v2
EMBEDDING_DIM=1024
# EMBEDDING_LOCAL_PATH=./models/Xorbits/bge-m3